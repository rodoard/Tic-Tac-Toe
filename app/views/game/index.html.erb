<% content_for :howto do %>
<%= render :partial => 'howto' %>
<% end %>

<%= form_tag url_for(:action=>'move'), {:id=>'game-form',:method=>'get',:remote=>true} do %>
<div id="game-board">
<%= render :partial => 'game_table' %>

</div>

<div id="game-stats">
<%= render :partial => 'game_stats' %>
</div>

<%= hidden_field_tag "cid", '' %>
<%= hidden_field_tag "turn", 'human' %>
<% end %>